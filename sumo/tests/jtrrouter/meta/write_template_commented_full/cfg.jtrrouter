<?xml version="1.0" encoding="iso-8859-1"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.sf.net/xsd/jtrrouterConfiguration.xsd">

    <input>
        <!-- Use FILE as SUMO-network to route on -->
        <net-file value="" synonymes="n net"/>

        <!-- Read (additional) districts from FILE -->
        <districts value="" synonymes="d"/>

        <!-- Read alternatives from FILE -->
        <alternatives value="" synonymes="a"/>

        <!-- Read network weights from FILE(s) -->
        <weights value="" synonymes="w weight-files"/>

        <!-- Read lane-weights from FILE(s) -->
        <lane-weights value=""/>

        <!-- Name of the xml attribute which gives the edge weight -->
        <measure value="traveltime" synonymes="x"/>

        <!-- Read flow-definitions from FILE -->
        <flow-definition value="" synonymes="f flows"/>

        <!-- Read turn-definitions from FILE -->
        <turn-definition value="" synonymes="t turns"/>

    </input>

    <output>
        <!-- Write generated routes to FILE -->
        <output-file value="" synonymes="o output"/>

        <!-- Write exit times (weights) for each edge -->
        <exit-times value="false"/>

    </output>

    <processing>
        <!-- Continue if a route could not be build -->
        <continue-on-unbuild value="false"/>

        <!-- Assume input is unsorted -->
        <unsorted value="false"/>

        <!-- generate random departure times for flow input -->
        <randomize-flows value="false"/>

        <!-- Prune the number of alternatives to INT -->
        <max-alternatives value="5"/>

        <!-- Remove loops within the route; Remove turnarounds at start and end of the route -->
        <remove-loops value="false"/>

        <!-- Tries to correct a false route -->
        <repair value="false"/>

        <!-- Interpolate edge weights at interval boundaries -->
        <interpolate value="false"/>

        <!-- Use origin and destination zones (districts) for in- and output -->
        <with-taz value="false"/>

        <!--  -->
        <max-edges-factor value="2"/>

        <!-- Use STR as default turn definition -->
        <turn-defaults value="30,50,20" synonymes="T"/>

        <!-- Use STR as sink list -->
        <sinks value="" synonymes="s"/>

        <!--  -->
        <accept-all-destinations value="false" synonymes="A"/>

        <!--  -->
        <ignore-classes value="false" synonymes="i"/>

        <!-- Continue on broken input -->
        <dismiss-loading-errors value="false"/>

    </processing>

    <defaults>
        <!-- Assigns a default depart lane -->
        <departlane value=""/>

        <!-- Assigns a default depart position -->
        <departpos value=""/>

        <!-- Assigns a default depart speed -->
        <departspeed value=""/>

        <!-- Assigns a default arrival lane -->
        <arrivallane value=""/>

        <!-- Assigns a default arrival position -->
        <arrivalpos value=""/>

        <!-- Assigns a default arrival speed -->
        <arrivalspeed value=""/>

        <!-- Defaults will override given values -->
        <defaults-override value="false"/>

    </defaults>

    <time>
        <!-- Defines the begin time; Previous trips will be discarded -->
        <begin value="0" synonymes="b"/>

        <!-- Defines the end time; Later trips will be discarded; Defaults to the maximum time that SUMO can represent -->
        <end value="2147483" synonymes="e"/>

    </time>

    <report>
        <!-- Switches to verbose output -->
        <verbose value="false" synonymes="v"/>

        <!-- Prints option values before processing -->
        <print-options value="false" synonymes="p"/>

        <!-- Prints this screen -->
        <help value="false" synonymes="?"/>

        <!-- Prints the current version -->
        <version value="false" synonymes="V"/>

        <!-- Disables output of warnings -->
        <suppress-warnings value="false" synonymes="W"/>

        <!-- Writes all messages to FILE -->
        <log-file value="" synonymes="l"/>

        <!-- Defines how often statistics shall be printed -->
        <stats-period value="-1"/>

    </report>

    <random_number>
        <!-- Initialises the random number generator with the current system time -->
        <random value="false" synonymes="abs-rand"/>

        <!-- Initialises the random number generator with the given value -->
        <seed value="23423" synonymes="srand"/>

    </random_number>

</configuration>
