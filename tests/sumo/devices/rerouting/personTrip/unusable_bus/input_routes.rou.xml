<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <flow id="bus" from = "beg" to ="end" line="bus" begin="0" end="1000" period="300">
		<stop lane="beg_0" until="10"/>
		<stop lane="left_0" until="110"/>
		<stop lane="end_0" until="210"/>
	</flow>
    <person id="p0" depart="0">
        <personTrip from="beg" to="end" modes="public"/>
    </person>
    <person id="p1" depart="0">
        <personTrip from="end" to="beg" modes="public"/>
    </person>
</routes>
