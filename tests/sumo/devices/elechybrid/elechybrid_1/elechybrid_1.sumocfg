<?xml version="1.0" encoding="UTF-8"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">

    <input>
        <net-file value="elechybrid_1.net.xml"/>
        <route-files value="trolleybus.rou.xml"/>
        <additional-files value="busstop.add.xml,overheadwires.add.xml"/>
    </input>
    
    <time>
        <end value="100"/>
    </time>
    
    <processing>
         <overhead-wire-solver value="true"/>
    </processing>
    
    <elechybrid_device>
         <device.elechybrid.explicit value="test1,test3"/>
    </elechybrid_device>
    
    <output>
        <elechybrid-output value="elechybrid_out.xml"/>
        <elechybrid-output.aggregated value="true"/>
        <elechybrid-output.precision value="4"/>
        <overheadwiresegments-output value="overheadwires_out.xml"/>
        <substations-output value="substation_out.xml"/>      
    </output>
    
</configuration>
