<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <vType sigma="0" id="v1" probability="2"/>
    <vType sigma="0" id="v2" probability="1"/>
    <vTypeDistribution id="dist1" vTypes="v1 v2"/>
    <flow id="1" begin="0" period="2" number="10" type="dist1">
        <route edges="beg end"/>
    </flow>
    <vTypeDistribution id="dist2" vTypes="v1 v2" probabilities="0.8 0.2"/>
    <flow id="2" begin="0" period="2" number="10" type="dist2">
        <route edges="beg end"/>
    </flow>

    <vTypeDistribution id="dist3">
        <vType sigma="0" id="v1.1" probability="0.5"/>
        <vType sigma="0" id="v1.2" probability="0.5"/>
    </vTypeDistribution>
    <vTypeDistribution id="dist4">
        <vType sigma="0" id="v2.1" probability="1"/>
        <vType sigma="0" id="v2.2" probability="1"/>
    </vTypeDistribution>

    <vTypeDistribution id="dist5" vTypes="dist3 dist4" probabilities="0.8 0.2"/>
    <flow id="3" begin="30" period="2" number="100" type="dist5">
        <route edges="beg end"/>
    </flow>
</routes>
