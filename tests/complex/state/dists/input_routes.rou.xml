<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/routes_file.xsd">
    <route id="r1" edges="4/1to3/1 3/1to2/1 2/1to1/1 1/1to0/1"/>
    <routeDistribution id="r">
        <route edges="3/1to2/1 2/1to1/1 1/1to0/1"/>
        <route edges="2/1to1/1 1/1to0/1"/>
    </routeDistribution>
    <vTypeDistribution id="v">
        <vType id="v1" accel="1.8" decel="4.5" sigma="0" length="3" minGap="2" maxSpeed="10" probability=".4"/>
        <vType id="v2" accel="2.8" decel="4.5" sigma="0" length="3" minGap="2" maxSpeed="10" probability=".6"/>
    </vTypeDistribution>
    <vehicle id="0" depart="0" departSpeed="10" departPos="100" route="r1" type="v"/>
    <vehicle id="1" depart="5" departSpeed="10" departPos="100" route="r1" type="v"/>
    <flow id="f" begin="30" period="3" end="90" departSpeed="avg" route="r1" type="v"/>

</routes>
