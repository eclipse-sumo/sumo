    <taz id="TAZ_0" color="green">

        <!-- valid generic parameter (valid) -->
        <param key="key1" value="value1"/>

        <!-- empty generic parameter (invalid -->
        <param/>

        <!-- missin key (invalid) -->
        <param value="value2"/>

        <!-- empty key (invalid) -->
        <param key="" value="value3"/>

        <!-- key contains invalid characters (invalid) -->
