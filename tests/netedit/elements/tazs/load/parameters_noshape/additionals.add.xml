        <!-- value contains invalid characters (valid - no restrictions on value) -->
        <param key="key3" value="$$value with spaces%%"/>

         <!-- valid generic parameter (valid) -->
         <param key="keyFinal" value="valueFinal"/>

    </taz>
</additionals>
