<additional xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/additional_file.xsd">

    <!-- default route -->
    <route id="CRoute" edges="center0 center1"/>

    <!-- Calibrator -->
    <calibrator id="calibratorFlows" edge="center0" pos="20.00" period="100.00" output="calibratorFlows.txt">

        <!-- non valid (empty) -->
        <flow route="CRoute" begin="000" end="100" speed="10" vehsPerHour="250.00" id="CFlow01"/>

        <!-- non valid (dummy) -->
        <flow route="CRoute" begin="100" end="200" speed="10" vehsPerHour="250.00" id="CFlow02" arrivalSpeed="dummyArrivalSpeed"/>

        <!-- non valid (negative) -->
        <flow route="CRoute" begin="200" end="300" speed="10" vehsPerHour="250.00" id="CFlow03" arrivalSpeed="-12"/>

        <!-- valid A -->
        <flow route="CRoute" begin="300" end="400" speed="10" vehsPerHour="250.00" id="CFlow04" arrivalSpeed="3.5"/>

        <!-- valid B -->
        <flow route="CRoute" begin="400" end="500" speed="10" vehsPerHour="250.00" id="CFlow05" arrivalSpeed="current"/>

    </calibrator>

</additional>
