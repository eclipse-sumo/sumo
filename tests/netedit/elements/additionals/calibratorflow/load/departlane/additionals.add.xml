<additional xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/additional_file.xsd">

    <!-- default route -->
    <route id="CRoute" edges="EdgeCenter0 EdgeCenter1"/>

    <!-- Calibrator -->
    <calibrator id="calibratorFlows" edge="EdgeCenter0" pos="20.00" period="100.00" output="calibratorFlows.txt">

        <!-- non valid (empty) -->
        <flow route="CRoute" begin="000" end="100" speed="10" vehsPerHour="250.00" id="CFlow01"/>

        <!-- non valid (dummy) -->
        <flow route="CRoute" begin="100" end="200" speed="10" vehsPerHour="250.00" id="CFlow02" />

        <!-- valid -->
        <flow route="CRoute" begin="200" end="300" speed="10" vehsPerHour="250.00" id="CFlow03" />

    </calibrator>

</additional>
