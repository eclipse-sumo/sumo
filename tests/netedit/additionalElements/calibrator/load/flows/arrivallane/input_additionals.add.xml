<additional xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/additional_file.xsd">
	
	<!-- default route -->
	<route id="CRoute" edges="gneE0 gneE1"/>

	<!-- default vehicle type -->
	<vType id="CType"/>

	<!-- Calibrator -->
	<calibrator id="calibratorFlows" edge="gneE0" pos="20.00" freq="100.00" output="calibratorFlows.txt">

		<!-- non valid (empty) -->
		<flow type="CType" route="CRoute" begin="0" end="100" speed="10" id="CFlow01"/>
		
		<!-- non valid (dummy) -->
		<flow type="CType" route="CRoute" begin="0" end="100" speed="10" id="CFlow02" arrivalLane="dummyArrivalLane"/>
		
		<!-- non valid (negative) -->
		<flow type="CType" route="CRoute" begin="0" end="100" speed="10" id="CFlow03" arrivalLane="-3"/>
		
		<!-- valid -->
		<flow type="CType" route="CRoute" begin="0" end="100" speed="10" id="CFlow04" arrivalLane="1"/>
		
	</calibrator>
	
</additional>
