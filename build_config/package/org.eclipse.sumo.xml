<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/sumo-configuration">
    <sub-class-of type="application/xml"/>
    <comment>Sumo Configuration File</comment>
    <comment xml:lang="de">Sumo Konfigurationsdatei</comment>
    <glob pattern="*.sumocfg" />
  </mime-type>
  <mime-type type="application/netedit-configuration">
    <sub-class-of type="application/xml"/>
    <comment>Sumo Netedit Configuration File</comment>
    <comment xml:lang="de">Sumo Netedit Konfigurationsdatei</comment>
    <glob pattern="*.netecfg" />
  </mime-type>
</mime-info>
